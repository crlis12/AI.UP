{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@langchain/core": "^0.3.68",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --cache --write .",
    "format:check": "prettier --check .",
    "format:js": "prettier --cache --write \"src/**/*.{js,jsx}\"",
    "format:check:js": "prettier --check \"src/**/*.{js,jsx}\"",
    "deploy": "npm run build && swa deploy ./build --app-name ai-up-frontend --resource-group ai-up-resource-group",
    "deploy:build-only": "npm run build",
    "deploy:upload-only": "swa deploy ./build --app-name ai-up-frontend --resource-group ai-up-resource-group"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
